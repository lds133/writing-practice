{
  "title": "Wierszyki",
  "language": "pl-PL",
  "notes": "O literze ż i rz",
  "tags": [
    "Słupsk","szkoła" 
  ],
  "data": [
 
 
 
    {
      "text": "Niech Twoja główka malutka"
    },
    {
      "text": "zapamięta te krótkie słówka:"
    },
    {
      "text": "żaba, jeż, wąż, żmija,"
    },
    {
      "text": "a żółw z żalem Cię omija."
    },
    {
      "text": "Rzodkiewka chociaż mała,"
    },
    {
      "text": "do śniadania doskonała"
    },
    {
      "text": "Możesz schrupać ją bez chleba."
    },
    {
      "text": "i poszukaj w jej nazwie imienia,"
    },
    {
      "text": "gdy będzie taka potrzeba."
    }
 
 
  ]
}